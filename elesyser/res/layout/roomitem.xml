<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
  <RelativeLayout
		android:layout_width="fill_parent"
		android:drawingCacheQuality="high"
		android:layout_height="wrap_content">
    	<TextView android:id="@+id/tv_building"
    		  android:layout_width="120dip"
    		  android:layout_height="wrap_content"
    		  android:textSize="15sp"
    		  android:padding="5dip"></TextView>
    	<TextView android:id="@+id/tv_roomid"
    		  android:layout_width="120dip"
    		  android:layout_height="wrap_content"
    		  android:textSize="15sp"
    		  android:padding="5dip"
    		  android:layout_toRightOf="@+id/tv_building"
    		  android:layout_alignTop="@+id/tv_building"
    		  android:layout_alignBottom="@+id/tv_building"></TextView>
    	<TextView android:id="@+id/tv_seatsnum"
    		  android:layout_width="wrap_content"
    		  android:layout_height="wrap_content"
    		  android:textSize="15sp"
    		  android:padding="5dip"
    		  android:layout_toRightOf="@+id/tv_roomid"
    		  android:layout_alignTop="@+id/tv_building"
    		  android:layout_alignBottom="@+id/tv_building"></TextView>
  </RelativeLayout>
</LinearLayout>
